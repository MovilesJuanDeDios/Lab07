<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.casca.appcompat">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.Banda"
            android:label="@string/title_activity_navigation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Discografia"
            android:label="@string/title_activity_discografia"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activity.Video"
            android:label="@string/title_activity_video"
            android:screenOrientation="landscape">

        </activity>
        <activity
            android:name=".Activity.Canciones"
            android:label="@string/title_activity_canciones"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>